<div class="box" id='new_maps'>
<h3>Recently created</h3>

<ul>
<% for map in Map.new_maps %>
  <li><a href="/map/view/<%= map.name %>"><%= map.name %></a> by <a href="/author/<%= map.author %>"><%= map.author %></a> <%= time_ago_in_words(map.created_at) %> ago; <%= map.warpables.length %> images </li>
<% end %>
</ul>

</div>
